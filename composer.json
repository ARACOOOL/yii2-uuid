{
  "name": "aracoool/yii2-uuid",
  "description": "Yii 2 UUID Extension. A PHP 7.0+ library for generating RFC 4122 version 3, 4, and 5 universally unique identifiers (UUID).",
  "keywords": [
    "yii",
    "yii2",
    "uuid",
    "guid"
  ],
  "type": "yii2-extension",
  "homepage": "https://github.com/ARACOOOL/yii2-uuid",
  "license": "MIT",
  "authors": [
    {
      "name": "Askar Fuzaylov",
      "email": "tkdforever@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "yiisoft/yii2": "*"
  },
  "autoload": {
    "psr-4": {
      "aracoool\\uuid\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6"
  }
}